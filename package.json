{
  "private": true,
  "scripts": {
    "build": "./node_modules/.bin/webpack --config node_modules/laravel-mix/setup/webpack.config.js",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "restart": "php artisan migrate:fresh && php artisan passport:install && php artisan passport:client --personal"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18",
    "bootstrap": "^4.0.0",
    "cross-env": "^5.1",
    "css-loader": "^2.1.0",
    "jquery": "^3.2",
    "laravel-mix": "^4.0.7",
    "lodash": "^4.17.13",
    "popper.js": "^1.12",
    "react": "^16.8.3",
    "react-dom": "^16.2.0",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.15.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "dependencies": {
    "@cantonjs/react-scroll-view": "^0.4.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@trendmicro/react-sidenav": "^0.4.5",
    "dateformat": "^3.0.3",
    "mdbreact": "^4.11.0",
    "react-bootstrap-modal": "^4.2.0",
    "react-date-picker": "^7.2.0",
    "react-responsive-modal": "^3.6.0",
    "react-router-dom": "^4.3.1",
    "react-svg-radar-chart": "^1.2.0"
  }
